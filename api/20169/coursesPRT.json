    "PRT219Y1-Y-20169": {
        "courseId": "1961",
        "org": "SPA",
        "orgName": "Spanish (SPA) (PRT) (LAS)",
        "courseTitle": "Academic Portuguese",
        "code": "PRT219Y1",
        "courseDescription": "<p><span style=\"text-indent: 0cm; font-size: 10px;\">Meets the needs of students who have had exposure to spoken Portuguese in an informal context (living in a Portuguese speaking country, or in a Portuguese speaking family) but little to no exposure to written Portuguese. The course reviews English/ Portuguese spelling differences; written and spoken registers of Portuguese, and basic aspects of the grammatical system. The course &nbsp;(1) provides students with the essential understanding of Portuguese grammatical system; (2) builds their vocabulary; and (3) trains them to express themselves formally in both spoken and written Portuguese.</span></p>",
        "prerequisite": "Basic to relatively high ability to speak and understand Portuguese; limited or no formal education in Portuguese.",
        "corequisite": "",
